{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  customManagers: [
    {
      customType: "regex",
      description: "Process common lib json schema references",
      fileMatch: ["\\.json$"],
      matchStrings: [
        "https:\\/\\/raw.githubusercontent.com\\/TilloTech\\/tillo-platform-helm-library\\/(?<depName>\\S+?)-(?<currentValue>[\\d\\.]+?)\\/\\S+",
      ],
      datasourceTemplate: "helm",
      versioningTemplate: "semver",
      registryUrlTemplate: "https://TilloTech.github.io/tillo-platform-helm-library"
    },
  ],
}
